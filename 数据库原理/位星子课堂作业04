DROP TABLE TEACHER;;/*SkipError*/
CREATE TABLE TEACHER(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    T_ID VARCHAR(32) NOT NULL   COMMENT '教工号' ,
    T_NAME VARCHAR(32)    COMMENT '教工名' ,
    PRIMARY KEY (T_ID)
) COMMENT = '教师 ';;

ALTER TABLE TEACHER ADD UNIQUE T_INDEX(T_ID);;
ALTER TABLE TEACHER COMMENT '教师';;

DROP TABLE STUDENT;;/*SkipError*/
CREATE TABLE STUDENT(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    S_ID VARCHAR(32) NOT NULL   COMMENT '学生号' ,
    S_NAME VARCHAR(32)    COMMENT '学生名' ,
    PRIMARY KEY (S_ID)
) COMMENT = '学生 ';;

ALTER TABLE STUDENT ADD UNIQUE S_INDEX(S_ID);;
ALTER TABLE STUDENT COMMENT '学生';;

DROP TABLE TEACHER_LECTURER;;/*SkipError*/
CREATE TABLE TEACHER_LECTURER(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    T_ID VARCHAR(32) NOT NULL   COMMENT '教工号' ,
    PRIMARY KEY (T_ID)
) COMMENT = '普通教师 ';;

ALTER TABLE TEACHER_LECTURER COMMENT '普通教师';;

DROP TABLE TEACHER_TUTOR;;/*SkipError*/
CREATE TABLE TEACHER_TUTOR(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    T_ID VARCHAR(32) NOT NULL   COMMENT '教工号' ,
    PRIMARY KEY (T_ID)
) COMMENT = '指导教师 ';;

ALTER TABLE TEACHER_TUTOR COMMENT '指导教师';;

DROP TABLE FINAL_PROJECT;;/*SkipError*/
CREATE TABLE FINAL_PROJECT(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    P_ID VARCHAR(32) NOT NULL   COMMENT '项目号' ,
    P_NAME VARCHAR(32)    COMMENT '项目名' ,
    PRIMARY KEY (P_ID)
) COMMENT = '毕业设计 ';;

ALTER TABLE FINAL_PROJECT ADD UNIQUE P_INDEX(P_ID);;
ALTER TABLE FINAL_PROJECT COMMENT '毕业设计';;

DROP TABLE TEACHING;;/*SkipError*/
CREATE TABLE TEACHING(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    C_ID VARCHAR(32) NOT NULL   COMMENT '课程号' ,
    T_ID VARCHAR(32)    COMMENT '教工号' ,
    PRIMARY KEY (C_ID)
) COMMENT = '讲授 ';;

ALTER TABLE TEACHING COMMENT '讲授';;

DROP TABLE LEARNING;;/*SkipError*/
CREATE TABLE LEARNING(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    C_ID VARCHAR(32) NOT NULL   COMMENT '课程号' ,
    S_ID VARCHAR(32)    COMMENT '学号' ,
    PRIMARY KEY (C_ID)
) COMMENT = '修读 ';;

ALTER TABLE LEARNING COMMENT '修读';;

DROP TABLE MANAGING;;/*SkipError*/
CREATE TABLE MANAGING(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    T_ID VARCHAR(32) NOT NULL   COMMENT '教工号' ,
    S_ID VARCHAR(32)    COMMENT '学生号' ,
    PRIMARY KEY (T_ID)
) COMMENT = '管理 ';;

ALTER TABLE MANAGING COMMENT '管理';;

DROP TABLE TUTORING;;/*SkipError*/
CREATE TABLE TUTORING(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    T_ID VARCHAR(32)    COMMENT '教工号' ,
    P_ID VARCHAR(32) NOT NULL   COMMENT '项目号' ,
    PRIMARY KEY (P_ID)
) COMMENT = '指导 ';;

ALTER TABLE TUTORING COMMENT '指导';;

DROP TABLE PARTICIPATING;;/*SkipError*/
CREATE TABLE PARTICIPATING(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    P_ID VARCHAR(32) NOT NULL   COMMENT '项目号' ,
    P_NAME VARCHAR(32)    COMMENT '项目名' ,
    PRIMARY KEY (P_ID)
) COMMENT = '参加 ';;

ALTER TABLE PARTICIPATING COMMENT '参加';;

DROP TABLE COURSE;;/*SkipError*/
CREATE TABLE COURSE(
    CREATED_TIME DATETIME    COMMENT '创建时间' ,
    C_ID VARCHAR(32) NOT NULL   COMMENT '课程号' ,
    C_NAME VARCHAR(32)    COMMENT '课程名' ,
    PRIMARY KEY (C_ID)
) COMMENT = '课程 ';;

ALTER TABLE COURSE ADD UNIQUE C_INDEX(C_ID);;
ALTER TABLE COURSE COMMENT '课程';;
